<% include partials/header %>


<div class="container login">
<form action="/register" method="post">
  <div class="form-group">
    <label>Email Adresiniz</label>
    <input type="text" class="form-control" name="email"  placeholder="E-mail">
  </div>
  <div class="form-group">
    <label>Şifreniz</label>
    <input type="password" class="form-control" name="password"  placeholder="Şifreniz">
  </div>
  <div class="form-group">
    <label>Adınız</label>
    <input type="text" class="form-control" name="user[name]"  placeholder="Ad">
  </div>
  <div class="form-group">
    <label>Soyadınız</label>
    <input type="text" class="form-control" name="user[surname]"  placeholder="Soyad">
  </div>
  <div class="form-group">
    <label>T.C. Kimlik Numaranız</label>
    <input type="text" class="form-control" name="user[tc]"  placeholder="T.C.">
  </div>
  <div class="form-group">
    <label>Çalıştığınız Şirket</label>
    <input type="text" class="form-control" name="user[company]"  placeholder="Şirket">
  </div>
  <div class="form-group">
    <label>Telefonunuzun Bluetooth MAC Adresi</label>
    <input type="text" class="form-control" name="user[macAddress]"  placeholder="MAC Adresi">
  </div>
  <button type="submit" class="btn btn-primary">Gönder</button>
</form>
</div>


<% include partials/footer %>