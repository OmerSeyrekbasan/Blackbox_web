<% include ../partials/header %>


<div class="container login">
<form action="/callCenter" method="post">
  <div class="form-group">
    <label>Kullanıcının Email Adresi</label>
    <input type="text" class="form-control" name="email"  placeholder="E-mail">
  </div>
  <div class="form-group">
    <label>Kullanıcının Şifresi</label>
    <input type="password" class="form-control" name="password"  placeholder="Şifre">
  </div>
  <div class="form-group">
    <label>Kullanıcının Adı</label>
    <input type="text" class="form-control" name="user[name]"  placeholder="Ad">
  </div>
  <div class="form-group">
    <label>Kullanıcının Soyadı</label>
    <input type="text" class="form-control" name="user[surname]"  placeholder="Soyad">
  </div>
  <div class="form-group">
    <label>Kullanıcının T.C. Kimlik Numarası</label>
    <input type="text" class="form-control" name="user[tc]"  placeholder="T.C.">
  </div>
  <button type="submit" class="btn btn-primary">Onayla</button>
</form>
</div>

<% include ../partials/footer %>