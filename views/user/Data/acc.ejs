<script>
//İVMEÖLÇER
accX = [];
accY = [];
accZ = [];
accT = [];
accData = <%-JSON.stringify(accData) %>

accData.forEach(function(row) {
  accX.push(row.x);
  accY.push(row.y);
  accZ.push(row.z);
  accT.push(row.t);
}); 


var xAxis = {
  x: accT,
  y: accX,
  type: 'scatter',
  name: "X Axis"
};

var yAxis = {
  x: accT,
  y: accY,
  type: 'scatter',
  name: "Y Axis"
};

var zAxis = {
  x: accT,
  y: accZ,
  type: 'scatter',
  name: "Z Axis"
};

var data = [xAxis, yAxis, zAxis];

var layout = {
  title: 'İvmeölçer Grafiği',
  autosize: false,
  width: 1200,
  height: 500,
  xaxis: {
    titlefont: {
      family: 'Courier New, monospace',
      size: 18,
      color: '#7f7f7f'
    }
  },
  yaxis: {
    titlefont: {
      family: 'Courier New, monospace',
      size: 18,
      color: '#7f7f7f'
    }
  }
};


Plotly.newPlot('myDiv', data, layout);

</script>


